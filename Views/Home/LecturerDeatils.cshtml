@model List<prog_poe_st10249266.Models.UserTBL>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@

<h1>Lecturer Details</h1>

<table>
    <thead>
        <tr>
            <th>User ID</th>
            <th>User Surname</th>
            <th>User Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserID</td>
                <td>@user.Surname</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
